{{#warning-message}}
  <div class="layout-row layout-align-space-between-center">
    <div>
      {{paper-icon "error"}}
      Klant {{#if duplicate.printPrefix}}{{duplicate.prefix}}{{/if}} {{duplicate.name}} {{#if duplicate.printSuffix}}{{duplicate.suffix}} {{/if}}heeft dit BTW-nummer al.
    </div>
    <div>
      {{paper-icon "forward"}}
    </div>
    {{#paper-tooltip}}Ga naar klant{{/paper-tooltip}}
  </div>
{{/warning-message}}
